<?xml version="1.0" encoding="utf-8"?>
<!--suppress ALL -->
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/app_bar_events"
    tools:context="com.example.rajeet.solutions2016.EventsActivity">
<android.support.v7.widget.CardView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/CVachieve">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:src="@drawable/programmer"
        android:id="@+id/IVprogrammer"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ACHIEVE"
        android:layout_gravity="center_horizontal"/>

</android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:paddingTop="8dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/CVpursue"
        android:layout_below="@+id/CVachieve"
        >
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/pursue"
            android:id="@+id/imageView4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PURSUE"
            android:layout_gravity="center_horizontal"/>

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:paddingTop="8dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/CVcompete"
        android:layout_below="@+id/CVpursue">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="10dp"
            android:text="COMPETE"
            android:layout_marginRight="400dp"
            android:layout_gravity="center_horizontal"/>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/compete"
            android:id="@+id/imageView3" />

    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:paddingTop="8dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_marginTop="39dp"
        android:layout_below="@+id/CVcompete"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/CVspark">

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/spark"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="10dp"
            android:text="SPARK"
            android:layout_marginRight="400dp"
            android:layout_gravity="center_horizontal"/>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:paddingTop="8dp"
        android:paddingLeft="2dp"
        android:paddingRight="2dp"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_below="@+id/CVspark"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="57dp"
        >

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:src="@drawable/photo"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="10dp"
            android:text="EXCEL"
            android:layout_marginRight="400dp"
            android:layout_gravity="center_horizontal"/>
    </android.support.v7.widget.CardView>
</RelativeLayout>
